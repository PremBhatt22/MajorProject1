<% if(success &&  success.length) { %>
    <div style="display: flex; justify-content:  space-between;" class="alert alert-success alert-dismmissible fade show col-3 offset-4" role="alert">
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close"></button>
    </div>
<% } %>

 
<% if(error &&  error.length) { %>
    <div  style="display: flex; justify-content:  space-between;" class="alert alert-danger alert-dismmissible fade show col-3 offset-4" role="alert">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close"></button>
    </div>
<% } %>

 
